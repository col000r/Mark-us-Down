{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main",
    "doc-*"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "core:window:allow-set-title",
    "core:window:allow-close",
    "core:window:allow-center",
    "core:webview:allow-create-webview-window",
    "shell:allow-open"
  ]
}
